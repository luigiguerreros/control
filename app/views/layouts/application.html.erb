<!DOCTYPE html>
<html>
<head>
  <title>Control</title>

  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<%= render "layouts/header" %>
<body>

<div class="row">
	<div class="col-md-3">
		<%= render "layouts/izquierda" %>
	</div>
	<div class="col-md-9">
		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>
		<%= yield %>
	</div>
</div>


</body>
</html>
